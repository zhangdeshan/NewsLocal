<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.news.admin.model">
	<class name="AdminPower" table="admin_power">
		<id name="Power_Id">
			<generator class="native" />
		</id>
		<many-to-one name="Admin_Id" column="Admin_Id"/>
		<many-to-one name="PowerGroup_Id" column="PowerGroup_Id"/>
	</class>
</hibernate-mapping>
